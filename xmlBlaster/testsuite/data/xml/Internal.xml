<!-- xmlBlaster.org Demo -->
<!-- Example how to query internal state of xmlBlaster -->


<!-- Exapmples how to subscribe to MessageUnits: -->

<!-- Accessing the current xmlBlaster allocated memory
        java DomQueryTest Internal.xml "xmlBlaster/key[@oid='__sys__Login']" -->

<!-- Accessing all xmlBlaster internal data
        java DomQueryTest Internal.xml xmlBlaster/key/__sys__internal -->

<xmlBlaster>

   <key oid='__sys__Login'>
      <__sys__internal>
      </__sys__internal>
   </key>

   <key oid='__cmd:?freeMem'>
   </key>

</xmlBlaster>


