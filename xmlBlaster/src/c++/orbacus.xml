<?xml version="1.0"?>
<project default="all" basedir=".">

  <property name="corba.impl"   value="ORBACUS"/>
  <property name="corba.dir"    value="${src.dir}/generated"/>
  <property name="jthread.home" value="/usr/local/jthread"/>
  <property name="jthread.inc"  value="-I${jthread.home}/include"/>
  <property name="jthread.libs" value="-L${jthread.home}/lib -lJTC"/>
  <property name="corba1.inc"   value="-I/usr/local/orbacus/include"/>
  <property name="corba.inc"    value="${jthread.inc} -I${corba.dir} ${corba1.inc} -D${corba.impl}"/>
  <property name="corba.libs"   value="${jthread.libs} -L${corba.home}/lib -lOB -lpthread" />
  <property name="cos.libs"     value="-L${corba.home}/lib -lCosNaming"/>
  <property name="corba.obj"    value="obj/xmlBlaster.o obj/xmlBlaster_skel.o"/>
  <property name="idl.cmd"      value="${corba.home}/bin/idl "/>
  <property name="idl.flag"     value=""/>
  <target name="all">
    <echo message="configured for ORBACUS"/>
    <ant dir="." target="${subtarget}"/>
  </target>
 
</project>


