CORBA_IMPL   =ORBACUS
CORBA_DIR    =$(SRC_DIR)/generated
JTHREAD_HOME =/usr/local/jthread
JTHREAD_INC  =-I$(JTHREAD_HOME)/include
JTHREAD_LIB  =-L$(JTHREAD_HOME)/lib -lJTC
CORBA1_INC   =-I/usr/local/orbacus/include
CORBA_INC    =$(JTHREAD_INC) -I$(CORBA_DIR) $(CORBA1_INC) -D$(CORBA_IMPL)
CORBA_LIB    =$(JTHREAD_LIB) -L$(CORBACPP_HOME)/lib -lOB -lpthread 
COS_LIB      =-L$(CORBACPP_HOME)/lib -lCosNaming
CORBA_SRC    =$(CORBA_DIR)/xmlBlaster.h $(CORBA_DIR)/xmlBlaster.cpp\
	$(CORBA_DIR)/xmlBlaster_skel.h $(CORBA_DIR)/xmlBlaster_skel.cpp
CORBA_OBJ    =$(CORBA_DIR)/xmlBlaster.o $(CORBA_DIR)/xmlBlaster_skel.o
IDL_CMD      =$(CORBACPP_HOME)/bin/idl 
IDL_FLAG     =
