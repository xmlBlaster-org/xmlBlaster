<?xml version="1.0"?>
<project default="all" basedir=".">
  <property name="corba.impl"   value="MICO"/>
  <property name="mico.version" value="${corbacpp.ver}"/>
  <property name="corba.dir"    value="${src.dir}/generated"/>
  <property name="corba1.inc"   value="-I${corba.home}/include"/>
  <property name="corba.inc"    value="-I${corba.dir} ${corba1.inc} -D${corba.impl}"/>
  <property name="corba.libs"   value="-ldl -L${corba.home}/lib -lmico${mico.version} -lpthread" />
  <property name="cos.libs"     value="-L${corba.home}/lib -lmicocoss${mico.version}"/>
  <property name="corba.obj"    value="obj/xmlBlaster.o"/>
  <property name="idl.cmd"      value="${corba.home}/bin/idl"/>
  <property name="idl.flag"     value="--poa --no-boa"/>
  <target name="all">
    <echo message="configured for MICO"/>
    <ant dir="." target="${subtarget}"/>
  </target>

</project>


