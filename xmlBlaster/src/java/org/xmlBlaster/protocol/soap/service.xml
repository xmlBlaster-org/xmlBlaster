    <ws:service>
      <!-- Service URI -->
      <ws:uri>urn:AuthenticationService</ws:uri>
      <!-- Provider: Java, EJB -->
      <ws:type>Java</ws:type>
      <!-- Fully qualified Java Class -->
      <ws:class ws:static="false">org.xmlBlaster.protocol.soap.AuthenticateImpl</ws:class>
      <!-- Methods -->
      <ws:methods>
        <ws:method>connect</ws:method>
        <ws:method>disconnect</ws:method>
        <ws:method>ping</ws:method>
      </ws:methods>
    </ws:service>

