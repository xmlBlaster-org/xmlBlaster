<ws:deployment xmlns:ws="http://www.soapanywhere.org/saw10/">
  <ws:name>Math Service</ws:name>
  <!-- All of the mappings in this deployment -->
  <ws:mappings>
  </ws:mappings>
  
  <!-- All of the services in this deployment -->
