   export CLASSPATH=$CLASSPATH:$XMLBLASTER_HOME/lib/jaxen.jar
 
   java org.xmlBlaster.engine.mime.xpath.XPathFilter -inFile ClientProperty.xml -isQos true
   
   xpath > //clientProperty[@name='corrId']
   Match: true


See: http://www.xmlblaster.org/xmlBlaster/doc/requirements/mime.plugin.access.xpath.html
